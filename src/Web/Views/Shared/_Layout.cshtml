<!DOCTYPE html>
<html lang="ru">



<head>

    <meta name="description" content="Автомат с напитками для Интравижн от Матиева.">
    <meta name="keywords" content="Автомат напиткм Интравижн Матиев Вода.">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Web</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Web.styles.css" asp-append-version="true" />

    @RenderSection("Head", required: false)

</head>

<body>
        <main role="main" class="pb-3">
            @RenderSection("Body", required: true)
        </main>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("ScriptJs", required: false)

</body>

</html>
